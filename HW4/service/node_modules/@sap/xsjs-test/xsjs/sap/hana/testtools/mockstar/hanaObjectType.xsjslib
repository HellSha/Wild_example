/*jslint nomen:true*/
var HanaObjectType = function(typeName) {
	var HanaObjectTypes = $.import("sap.hana.testtools.mockstar", "hanaObjectTypes").HanaObjectTypes;
	this._type = typeName;
	
	this.isOfType = function(types) {
		if (Array.isArray(types)) {
			return types.some(this.isOfType.bind(this));
		}
		return typeName === types;
	};
	
	this.isTableType = function() {
		return this.isOfType(HanaObjectTypes.tableTypes);
	};

	this.isProcedureType = function() {
		return this.isOfType(HanaObjectTypes.procedureTypes);
	};

	this.isViewType = function() {
		return this.isOfType(HanaObjectTypes.viewTypes);
	};
	
	this.isValid = function() {
		return this.isOfType(HanaObjectTypes.allTypes());
	};
	
	this.toString = function() {
		return typeName;
	};
	
	this.equals = function(other) {
		return typeName === other.toString();
	};

	this.match = function(otherTypeName) {
		return typeName === otherTypeName;
	};

	this.clone = function() {
		return new HanaObjectType(typeName);
	};

	

};