var HanaObjectTypes = {
	ATTRIBUTE_VIEW : "attributeView",
	ANALYTIC_VIEW : "analyticView",
	CALCULATION_VIEW : "calculationView",
	TABLE : "table",
	VIRTUAL_TABLE : "virtual",
	PROCEDURE : "procedure",
	HIERARCHY_VIEW : "hierarchyview",
	HDBPROCEDURE : "hdbprocedure",
	ODATA : "odata",
	HDBDD : "hdbdd",
	HDB_VIEW : "hdbview"
};

HanaObjectTypes.tableTypes = [ HanaObjectTypes.TABLE, HanaObjectTypes.HDBDD, HanaObjectTypes.VIRTUAL_TABLE ];
HanaObjectTypes.procedureTypes = [ HanaObjectTypes.PROCEDURE, HanaObjectTypes.HDBPROCEDURE ];
HanaObjectTypes.viewTypes = [ HanaObjectTypes.CALCULATION_VIEW, HanaObjectTypes.ATTRIBUTE_VIEW, HanaObjectTypes.ANALYTIC_VIEW, HanaObjectTypes.HDB_VIEW, HanaObjectTypes.HIERARCHY_VIEW ];
HanaObjectTypes.otherTypes = [HanaObjectTypes.ODATA ];

HanaObjectTypes.allTypes = function() {
	return Array.concat(HanaObjectTypes.tableTypes, HanaObjectTypes.procedureTypes, HanaObjectTypes.viewTypes, HanaObjectTypes.otherTypes );
};
