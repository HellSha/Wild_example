function AnalyticViewDependencySubstitutor() {
	var HanaObjectTypes = $.import("sap.hana.testtools.mockstar", "hanaObjectTypes").HanaObjectTypes;
	
	this.execute = function(hanaObject) {
		hanaObject.getObjectDependencies().forEach(function(dependency) {
			var originalTag = 'schemaName="' + dependency.oldLocation.getSchema() + '" columnObjectName="' + dependency.oldLocation.getName() + '"';
			var substituteTagPart = 'schemaName="' + dependency.getSchema() + '" columnObjectName="' + dependency.getName() + '"';

			hanaObject.cdata = hanaObject.cdata.replace(originalTag, substituteTagPart, "g");
			
			if( dependency.type.equals(dependency.oldType) && dependency.type.isOfType([HanaObjectTypes.ATTRIBUTE_VIEW]) ) {
				var originalName = dependency.oldLocation.getParsedName();
				originalTag = '/' + originalName.packageName + '/attributeviews/' + originalName.objectName;
				var substituteName = dependency.location.getParsedName();
				substituteTagPart = '/' + substituteName.packageName + '/attributeviews/' + substituteName.objectName;
				
				hanaObject.cdata = hanaObject.cdata.replace(originalTag, substituteTagPart, "g");
			}
		});
	};
}
